<%= simple_form_for(@diet) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :amount %>
    <%= f.input :datestart %>
    <%= f.input :dateend %>
    <%= f.association :animal %>
    <%= f.association :stock %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
