<div class="page-header">
  <%= link_to diets_path, class: 'btn btn-default' do %>
    <span class="fa fa-chevron-left"></span>
    <%= I18n.t('crud.back') %>
  <% end %>
  <%= link_to edit_diet_path(@diet), class: 'btn btn-primary' do %>
    <span class="fa fa-pencil"></span>
    <%= I18n.t('crud.edit') %>
  <% end %>
  <h3><%= I18n.t('crud.show') %> diet</h3>
</div>
<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
  <ul class="list-group">
    <li class="list-group-item">
      <strong>Amount:</strong>
      <%= @diet.amount %>
    </li>
    <li class="list-group-item">
      <strong>Datestart:</strong>
      <%= @diet.datestart %>
    </li>
    <li class="list-group-item">
      <strong>Dateend:</strong>
      <%= @diet.dateend %>
    </li>
    <li class="list-group-item">
      <strong>Animal:</strong>
      <%= @diet.animal %>
    </li>
    <li class="list-group-item">
      <strong>Stock:</strong>
      <%= @diet.stock %>
    </li>
  </ul>
</div>
