<%= simple_form_for(@treatment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :StartDate %>
    <%= f.input :EndDate %>
    <%= f.input :dosage %>
    <%= f.input :lack %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-success" %>
  </div>
<% end %>
<br>
