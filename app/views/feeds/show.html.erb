<div class="page-header">
  <%= link_to feeds_path, class: 'btn btn-default' do %>
    <span class="fa fa-chevron-left"></span>
    <%= I18n.t('crud.back') %>
  <% end %>
  <%= link_to edit_feed_path(@feed), class: 'btn btn-primary' do %>
    <span class="fa fa-pencil"></span>
    <%= I18n.t('crud.edit') %>
  <% end %>
  <h3><%= I18n.t('crud.show') %> feed</h3>
</div>
<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
  <ul class="list-group">
    <li class="list-group-item">
      <strong>Kind:</strong>
      <%= @feed.kind %>
    </li>
    <li class="list-group-item">
      <strong>Name:</strong>
      <%= @feed.name %>
    </li>
    <li class="list-group-item">
      <strong>Description:</strong>
      <%= @feed.description %>
    </li>
  </ul>
</div>
