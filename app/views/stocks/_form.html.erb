<%= simple_form_for(@stock) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :bin %>
    <%= f.association :feed %>
    <%= f.association :glebe %>
    <%= f.input :amount, as: :normal, before: "MÂ³" %>
    <%= f.input :datestock, as: :date_time_picker  %>
    <%= f.input :datestart, as: :date_time_picker %>
    <%= f.input :dateend, as: :date_time_picker %>
    <%= f.input :fournisseur %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
