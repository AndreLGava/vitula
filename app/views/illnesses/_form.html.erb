<%= simple_form_for(@illness) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :StartDate %>
    <%= f.input :EndDate %>
    <%= f.input :sequel %>
    <%= f.association :animal %>
    <%= f.association :disease %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-success" %>
  </div>
<% end %>
<br>
