<% breadcrumb :disease, @disease %>
<div class="page-header">
  <%= link_to diseases_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_disease_path(@disease), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show disease</h1>
</div>

<dl class="dl-horizontal">
  <dt>Vulgarname:</dt>
  <dd><%= @disease.VulgarName %></dd>

  <dt>Cientificname:</dt>
  <dd><%= @disease.CientificName %></dd>

  <dt>Causes:</dt>
  <dd><%= @disease.Causes %></dd>

  <dt>Description:</dt>
  <dd><%= @disease.Description %></dd>

  <dt>Symptoms:</dt>
  <dd><%= @disease.Symptoms %></dd>

</dl>
