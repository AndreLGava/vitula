<% breadcrumb :drug, @drug %>
<div class="page-header">
  <%= link_to drugs_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_drug_path(@drug), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show drug</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @drug.name %></dd>

  <dt>Laboratory:</dt>
  <dd><%= @drug.laboratory %></dd>

  <dt>Reference:</dt>
  <dd><%= @drug.reference %></dd>

  <dt>Presentation:</dt>
  <dd><%= @drug.presentation %></dd>

  <dt>Use:</dt>
  <dd><%= @drug.use %></dd>

  <dt>Composition:</dt>
  <dd><%= @drug.composition %></dd>

  <dt>Indication:</dt>
  <dd><%= @drug.indication %></dd>

  <dt>Contraindication:</dt>
  <dd><%= @drug.contraindication %></dd>

  <dt>Warning:</dt>
  <dd><%= @drug.warning %></dd>

  <dt>Interaction:</dt>
  <dd><%= @drug.interaction %></dd>

  <dt>Effect:</dt>
  <dd><%= @drug.effect %></dd>

  <dt>Dosage:</dt>
  <dd><%= @drug.dosage %></dd>

  <dt>Overdosage:</dt>
  <dd><%= @drug.overdosage %></dd>

  <dt>Properties:</dt>
  <dd><%= @drug.properties %></dd>

  <dt>Species:</dt>
  <dd><%= @drug.species %></dd>

</dl>
