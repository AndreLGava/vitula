var SplineGraph = {
  setup: function (data) {
    console.log(data.data.height);
      $('.chart').highcharts({
    			chart: {
    					type: 'spline'
    			},
    			title: {
    					text: data.title
    			},
    			subtitle: {
    					text: data.subtitle
    			},
    			xAxis: {
                        categories: data.data.created,
    					type: 'datetime',
    					labels: {
    							overflow: 'justify'
    					}
    			},
    			yAxis: {
    					title: {
    							text: data.yAxis
    					},
    					minorGridLineWidth: 0,
    					gridLineWidth: 0,
    					alternateGridColor: null    					
    			},
    			plotOptions: {
    					spline: {
    							lineWidth: 4,
    							states: {
    									hover: {
    											lineWidth: 5
    									}
    							},
    							marker: {
    									enabled: false
    							}
    					}
    			},
    			series: [{
    					name: 'Height',
    					data: data.data.height

    			}, {
    					name: 'Weight',
    					data: data.data.weight
    			}],
    			navigation: {
    					menuItemStyle: {
    							fontSize: '10px'
    					}
    			}
    	});
    }
};
